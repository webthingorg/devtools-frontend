{
  "description": "Testing devtools registration",
  "manifest_version": 2,
  "version": "1.0",
  "name": "Wasm CXX-DWARF Devtools Plugin",
  "background": {
    "scripts": ["DWARFSymbolsPlugin.js"],
    "persistent": false
  },
  "permissions": [
    "https://*/",
    "file://*/",
    "http://*/"
  ],
  "devtools_page": "DevToolsPlugin.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
