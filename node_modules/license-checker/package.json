  "dependencies": {
    "chalk": "^2.4.1",
    "debug": "^3.1.0",
    "mkdirp": "^0.5.1",
    "nopt": "^4.0.1",
    "read-installed": "~4.0.3",
    "semver": "^5.5.0",
    "spdx-correct": "^3.0.0",
    "spdx-expression-parse": "^3.0.0",
    "spdx-satisfies": "^4.0.0",
    "treeify": "^1.1.0"
  },
  "description": "Check license info for a package",
  "devDependencies": {
    "camden.jshint": "github:cfjedimaster/brackets-jshint",
    "detectionizr": "*",
    "eslint": "^4.19.1",
    "format-package-json": "^0.2.0",
    "git-contributors": "^0.2.3",
    "github-changes": "^1.1.2",
    "istanbul": "^0.4.3",
    "jenkins-mocha": "^6.0.0",
    "locale": "^0.1.0",
    "queue": "^4.4.2",
    "request": "^2.85.0",
    "rimraf": "^2.6.2"
  },
  "homepage": "https://github.com/davglass/license-checker#readme",
  "keywords": [
    "license",
    "cli",
    "checker",
    "oss"
  ],
  "license": "BSD-3-Clause",
  "main": "./lib/index.js",
  "name": "license-checker",
  "nyc": {
    "branches": 99,
    "check-coverage": true,
    "exclude": [
      "**/tests/*.js"
    ],
    "functions": 100,
    "lines": 99,
    "statements": 99
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/davglass/license-checker.git"
  },
  "scripts": {
    "changes": "github-changes -o davglass -r license-checker",
    "contrib": "./scripts/contrib.js",
    "posttest": "nyc check-coverage && nyc report -r lcov -r text -r text-summary",
    "pretest": "eslint --fix .",
    "test": "jenkins-mocha ./tests/*.js"
  },
  "version": "25.0.1"
}
