  "description": "Wrap words to a specified length.",
  "devDependencies": {
    "gulp-format-md": "^0.1.11",
    "mocha": "^3.2.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "homepage": "https://github.com/jonschlinkert/word-wrap",
  "keywords": [
    "break",
    "carriage",
    "line",
    "new-line",
    "newline",
    "return",
    "soft",
    "text",
    "word",
    "word-wrap",
    "words",
    "wrap"
  ],
  "license": "MIT",
  "main": "index.js",
  "name": "word-wrap",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonschlinkert/word-wrap.git"
  },
  "scripts": {
    "test": "mocha"
  },
  "typings": "index.d.ts",
  "verb": {
    "layout": "default",
    "lint": {
      "reflinks": true
    },
    "plugins": [
      "gulp-format-md"
    ],
    "reflinks": [
      "verb",
      "verb-generate-readme"
    ],
    "related": {
      "list": [
        "common-words",
        "shuffle-words",
        "unique-words",
        "wordcount"
      ]
    },
    "tasks": [
      "readme"
    ],
    "toc": false
  },
  "version": "1.2.3"
}
