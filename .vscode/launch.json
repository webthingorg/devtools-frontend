{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Run unit tests in debugger",
            "preLaunchTask": "1-build_Debug",
            "runtimeExecutable": "/usr/bin/npm",
            "runtimeArgs": ["run", "unittest"],
            "port": 9222,
            "timeout": 30000,
            "browserLaunchLocation": "workspace",
            "outputCapture": "std",
            "trace": true
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Run end2end tests in debugger",
            "preLaunchTask": "1-build_Debug",
            "runtimeExecutable": "/usr/bin/npm",
            "runtimeArgs": ["run", "e2etest"],
        },
    ]
}
