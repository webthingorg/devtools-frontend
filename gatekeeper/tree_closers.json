{
  "comment": ["This is a configuration file for gatekeeper_ng.py",
              "Look at that for documentation on this file's format."],
  "masters": {
    "https://build.chromium.org/p/client.devtools-frontend.integration": [
      {
        "builders": {
          "*": {
            "categories": [
              "devtools_all"
            ]
          }
        }
      }
    ]
  },

  "categories": {
    "devtools_all": {
      "closing_optional": [
        "*"
      ],
      "excluded_steps": [
        "webkit_layout_from_devtools (with patch)",
        "webkit_layout_from_devtools (retry shards with patch)",
        "webkit_layout_from_devtools (without patch)",
        "webkit_layout_tests (with patch)",
        "webkit_layout_tests (retry shards with patch)",
        "webkit_layout_tests (without patch)",
        "webkit_unit_tests_ng (with patch)",
        "webkit_unit_tests_ng (retry shards with patch)",
        "webkit_unit_tests_ng (without patch)",
        "Failure reason",
        "steps",
        "* (flakes)"
      ],
      "forgiving_optional": [
        "bot_update",
        "isolate tests",
        "package build",
        "extract build",
        "cleanup_temp",
        "gsutil upload",
        "taskkill"
      ],
      "status_template": "closed (\"%(unsatisfied)s\" on %(build_url)s \"%(builder_name)s\" from %(revision)s)"
    }
  }
}
